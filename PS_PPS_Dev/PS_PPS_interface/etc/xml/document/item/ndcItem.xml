<!-- NDC Item example  -->

<ndcItem
    xmlns="gov/va/med/pharmacy/peps/external/common/vo/outbound/item/ndcItem"
    xmlns:common="gov/va/med/pharmacy/peps/external/common/vo/outbound/common"
    xmlns:drugManufacturer="gov/va/med/pharmacy/peps/external/common/vo/outbound/ndf/drugManufacturer"
    xmlns:ndcUpn="gov/va/med/pharmacy/peps/external/common/vo/outbound/ndf/ndcUpn"
    xsi:schemaLocation="gov/va/med/pharmacy/peps/external/common/vo/outbound/item/ndcItem ../../schema/item/ndcItem.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    status="Approved"
    pepsIdNumber="1234567890">

    <drugManufacturerFile
        action="Add"
        number="55.95">
        <drugManufacturer:candidateKey>
            <drugManufacturer:name number="0.01">Bayer</drugManufacturer:name>
        </drugManufacturer:candidateKey>
    </drugManufacturerFile>

    <ndcUpnFile
        action="Add"
        number="50.67">
        <ndcUpn:candidateKey>
            <ndcUpn:ndc number="1">000677117201</ndcUpn:ndc>
        </ndcUpn:candidateKey>

        <ndcUpn:upn number="2">28492930</ndcUpn:upn>
        <ndcUpn:manufacturer number="3">Smithkline</ndcUpn:manufacturer>
        <ndcUpn:tradeName number="4">Aspirin</ndcUpn:tradeName>
        <ndcUpn:vaProductName number="5">Aspirin 81mg TAB</ndcUpn:vaProductName>
        <ndcUpn:packageSize number="8">OTHER</ndcUpn:packageSize>
        <ndcUpn:packageType number="9">OTHER</ndcUpn:packageType>
        <ndcUpn:otxRxIndicator number="10">R</ndcUpn:otxRxIndicator>
    </ndcUpnFile>
</ndcItem>
